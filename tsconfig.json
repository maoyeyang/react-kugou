{
  "compilerOptions": {
    "target": "es5",
    "baseUrl": "src",
    "paths": {
      "src/*": ["*"],
      "api/*": ["api/*"],
      "common/*": ["common/*"],
      "components/*": ["components/*"],
      "constants/*": ["constants/*"],
      "model/*": ["model/*"],
      "pages/*": ["pages/*"],
      "router/*": ["router/*"],
      "store/*": ["store/*"],
      "actions/*": ["store/actions/*"],
      "defaultStates/*": ["store/defaultStates/*"],
      "reducers/*": ["store/reducers/*"]
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "preserve"
  },
  "include": [
    "src"
  ]
}